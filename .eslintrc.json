{
  "extends": ["next", "plugin:prettier/recommended"],
  "plugins": ["react"],
  "rules": {
    "eqeqeq": "error",
    "no-console": "warn",
    "react/jsx-uses-vars": "error",
    "react/jsx-uses-react": "error",
    "prettier/prettier": "error"
  }
}
